#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/statemachine/work/usde-power-perp/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/statemachine/work/usde-power-perp/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/@graphql-codegen+cli@5.0.2_ixpq4asuuudncivzri26o3dmci/node_modules/@graphql-codegen/cli/cjs/bin.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/@graphql-codegen+cli@5.0.2_ixpq4asuuudncivzri26o3dmci/node_modules/@graphql-codegen/cli/cjs/bin.js" "$@"
fi
